[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/gagansp
[access "refs/for/refs/*"]
	read = group user/gagansp
	push = group user/gagansp
	label-Code-Review = -2..+2 group user/gagansp
	label-Verified = -1..+1 group user/gagansp
	rebase = group user/gagansp
	removeReviewer = group user/gagansp
	submit = group user/gagansp
[access "refs/heads/*"]
	read = group user/gagansp
	create = group user/gagansp
	push = +force group user/gagansp
	pushMerge = group user/gagansp
[access "refs/tags/*"]
	push = group user/gagansp
	push = +force group user/gagansp
